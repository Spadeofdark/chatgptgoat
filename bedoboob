local encodedURL = "aHR0cHM6Ly9yYXcuZ2l0aHVidXNlcmNvbnRlbnQuY29tL1NwYWRlb2ZkYXJrL2NoYXRncHRnb2F0L3JlZnMv
aGVhZHMvbWFpbi9iZWRvYm9vYg=="

local function b64Decode(data)
	local b='ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/'
	return (data:gsub('.', function(x)
		if x == '=' then return '' end
		local r,f='',(b:find(x)-1)
		for i=6,1,-1 do r=r..(f%2^i - f%2^(i-1) > 0 and '1' or '0') end
		return r
	end):gsub('%d%d%d?%d?%d?%d?%d?%d?', function(x)
		if #x ~= 8 then return '' end
		local c=0
		for i=1,8 do c=c + (x:sub(i,i)=='1' and 2^(8-i) or 0) end
		return string.char(c)
	end))
end

local function isValidSource(url, keyword)
	local success, result = pcall(function()
		return game:HttpGet(url, true)
	end)
	if not success or typeof(result) ~= "string" then return false end
	return result:find(keyword) ~= nil
end

local url = b64Decode(encodedURL)
if not isValidSource(url, "-- Final Safe GUI (By:@g4sser_)") then return end

-- GUI Starts Here
local CoreGui = game:GetService("CoreGui")
local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")
local TweenService = game:GetService("TweenService")
local LocalPlayer = Players.LocalPlayer

-- Lock core interfaces
pcall(function()
	StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.All, false)
end)

-- Try to freeze character movement
pcall(function()
	local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
	local hum = char:FindFirstChildOfClass("Humanoid")
	if hum then
		hum.WalkSpeed = 0
		hum.JumpPower = 0
	end
end)

-- Clear existing GUIs
for _, gui in ipairs(CoreGui:GetChildren()) do
	if gui:IsA("ScreenGui") then gui:Destroy() end
end

local gui = Instance.new("ScreenGui", CoreGui)
gui.IgnoreGuiInset = true
gui.ResetOnSpawn = false
gui.Name = "gasser_gui"
gui.DisplayOrder = 999999

-- Background beach image
local bg = Instance.new("ImageLabel", gui)
bg.Size = UDim2.new(1, 0, 1, 0)
bg.Image = "rbxassetid://17298301436"
bg.BackgroundTransparency = 1

-- Flying purple dragon
local dragon = Instance.new("ImageLabel", gui)
dragon.Size = UDim2.new(0.2, 0, 0.2, 0)
dragon.Position = UDim2.new(-0.2, 0, 0.05, 0)
dragon.BackgroundTransparency = 1
dragon.Image = "rbxassetid://17298317112"

TweenService:Create(dragon, TweenInfo.new(12, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut, -1, true),
	{ Position = UDim2.new(1.2, 0, 0.05, 0) }):Play()

-- Loading label
local loading = Instance.new("TextLabel", gui)
loading.Size = UDim2.new(1, 0, 0.1, 0)
loading.Position = UDim2.new(0, 0, 0.65, 0)
loading.BackgroundTransparency = 1
loading.TextColor3 = Color3.fromRGB(255, 255, 255)
loading.Font = Enum.Font.GothamBold
loading.TextScaled = true
loading.Text = "Loading.. 0%"

-- Progress bar
local bar = Instance.new("Frame", gui)
bar.Position = UDim2.new(0.25, 0, 0.8, 0)
bar.Size = UDim2.new(0.5, 0, 0.02, 0)
bar.BackgroundColor3 = Color3.fromRGB(100, 100, 255)
bar.BackgroundTransparency = 0.3

local fill = Instance.new("Frame", bar)
fill.BackgroundColor3 = Color3.fromRGB(180, 0, 255)
fill.Size = UDim2.new(0, 0, 1, 0)
fill.BorderSizePixel = 0

-- Watermark
local watermark = Instance.new("TextLabel", gui)
watermark.Size = UDim2.new(1, 0, 0.1, 0)
watermark.Position = UDim2.new(0, 0, 0.02, 0)
watermark.BackgroundTransparency = 1
watermark.Text = "By:@g4sser_"
watermark.Font = Enum.Font.GothamSemibold
watermark.TextColor3 = Color3.fromRGB(255, 255, 255)
watermark.TextScaled = true

-- Bypass text
local bypass = Instance.new("TextLabel", gui)
bypass.Size = UDim2.new(1, 0, 0.05, 0)
bypass.Position = UDim2.new(0, 0, 0.92, 0)
bypass.BackgroundTransparency = 1
bypass.Text = "Bypassing Anticheat..."
bypass.Font = Enum.Font.Code
bypass.TextColor3 = Color3.fromRGB(200, 200, 255)
bypass.TextScaled = true

-- Simulate loading
spawn(function()
	for i = 1, 100 do
		loading.Text = "Loading.. " .. i .. "%"
		fill:TweenSize(UDim2.new(i / 100, 0, 1, 0), "Out", "Quad", 0.05, true)
		task.wait(0.05)
	end

	bypass.Text = "Done. Unlocking UI..."

	-- Re-enable
	pcall(function()
		StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.All, true)
	end)
	pcall(function()
		local hum = LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
		if hum then
			hum.WalkSpeed = 16
			hum.JumpPower = 50
		end
	end)

	gui:Destroy()
end)
